#include <stdio.h>
int main()
{
    int a[100],i,n;
 printf("Enter size of the array:\n");
    scanf("%d", &n);
 printf("Enter elements in array:\n");
    for(i=0; i<n; i++)
    {
        scanf("%d",&a[i]);
    }
  printf("\n negative elements in array are : ");
    for(i=0; i<n; i++)
    {
         if(a[i] < 0)
        {
            printf("%d\t", a[i]);
        }
    }
 
    return 0;
}
